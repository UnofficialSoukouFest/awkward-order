{
	"name": "soukousai25",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "pnpm run '/^build:.*/'",
		"build:app": "react-router build",
		"build:scripts": "pnpm --filter @latimeria/latimeria-cli run build",
		"build:shared": "pnpm --filter @latimeria/shared run build",
		"dev": "pnpm run '/^dev:.*/'",
		"dev:app": "react-router dev",
		"dev:assets-proxy": "pnpm --filter assets-proxy run dev",
		"dev:scripts": "pnpm --filter @latimeria/latimeria-cli run dev",
		"dev:shared": "pnpm --filter @latimeria/shared run dev",
		"start": "react-router-serve ./build/server/index.js",
		"lint": "biome check",
		"format": "biome format --write",
		"generate": "pnpm run '/^generate:.*/'",
		"generate:cf-type": "wrangler types",
		"generate:react-router": "react-router typegen",
		"generate:css-type": "hcm 'app/**/*.module.css' -o 'app/generated'",
		"typecheck": "tsc --noEmit",
		"syncPublic": "latimeria gdrive",
		"db:sync": "drizzle-kit generate && drizzle-kit push"
	},
	"dependencies": {
		"@hono/valibot-validator": "catalog:",
		"@latimeria/design-token": "^0.2.0",
		"@latimeria/ganoine": "^1.1.1",
		"@latimeria/shared": "workspace:^",
		"@libsql/client": "^0.15.14",
		"@praha/drizzle-factory": "^1.2.1",
		"@react-router/node": "^7.8.1",
		"@react-router/serve": "^7.8.1",
		"@valibot/to-json-schema": "catalog:",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.44.4",
		"drizzle-valibot": "^0.4.2",
		"es-toolkit": "^1.39.10",
		"hono": "catalog:",
		"hono-openapi": "catalog:",
		"isbot": "^5.1.30",
		"jotai": "^2.13.1",
		"leaflet": "^1.9.4",
		"react": "^19.1.1",
		"react-aria-components": "^1.11.0",
		"react-dom": "^19.1.1",
		"react-leaflet": "^5.0.0",
		"react-router": "^7.8.1",
		"svg-parser": "^2.0.4",
		"svg-path-to-polygons": "^0.1.0",
		"uuid": "^11.1.0",
		"valibot": "catalog:",
		"vaul": "^1.1.2"
	},
	"devDependencies": {
		"@biomejs/biome": "catalog:",
		"@cloudflare/vite-plugin": "^1.11.6",
		"@iconify-json/mdi": "^1.2.3",
		"@latimeria/latimeria-cli": "workspace:^",
		"@react-router/dev": "^7.8.1",
		"@svgr/core": "^8.1.0",
		"@svgr/plugin-jsx": "^8.1.0",
		"@types/hast": "^3.0.4",
		"@types/leaflet": "^1.9.20",
		"@types/node": "catalog:",
		"@types/react": "^19.1.10",
		"@types/react-dom": "^19.1.7",
		"@types/svg-parser": "^2.0.6",
		"drizzle-kit": "^0.31.4",
		"happy-css-modules": "^4.0.0",
		"postcss": "^8.5.6",
		"sass": "^1.91.0",
		"typescript": "catalog:",
		"unplugin-icons": "^22.2.0",
		"vite": "^7.1.3",
		"vite-tsconfig-paths": "^5.1.4",
		"wrangler": "catalog:"
	}
